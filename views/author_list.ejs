<!DOCTYPE html>
<html>
  <head>
    <%- include('./partials/head', {title: title}); %>
  </head>
  <body>
    <%- include('./partials/sidebarNav'); %>
    <div class="col-sm-10">
      <h1><%= title %></h1>
      <ul>
        <% author_list.forEach( author => { %>
          <li>
            <a href="<%= author.url %>">
              <%= author.name %>
            </a>
            : (<%= author.formatted_date_of_birth %> - <%= author.formatted_date_of_death %>)
          </li>
        <% }); %>
      </ul>
    </div>
  </body>
</html>