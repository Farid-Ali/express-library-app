<!DOCTYPE html>
<html>
  <head>
    <%- include('./partials/head', {title: title}); %>
  </head>
  <body>
    <%- include('./partials/sidebarNav'); %>
    <div class="col-sm-10">
      <h1><%= title %></h1>
      <ul>
        <% book_list.forEach( book => { %>
          <li><a href="<%=book.url%>"><%= book.title %></a>: <%= book.author.name %></li>
        <% }); %>
      </ul>
    </div>
  </body>
</html>